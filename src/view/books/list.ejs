<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Book list</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Author</th>
            <th></th>
        </tr>
        <% data.forEach(item => { %>
            <tr>
                <td>1</td>
                <td><%= item.name %></td>
                <td><%= item.price %></td>
                <td>Author 1</td>
                <td><a href="/admin/books/<%= item.id_book %>/delete">Delete</a></td>
            </tr> 
        <% }) %>
    </table>
</body>
</html>